<setup>
  <selectFromSequence numberToSelect="2" withReplacement from="2" to="5" assignNames="a b" />
  <math name="coef1" simplify>3*($b-$a)</math>
  <math name="coef2" simplify>6*$a*$b</math>
  <function name="f" symbolic formula="2*x^3+$coef1*x^2-$coef2*x" />
  <math name="ans" unorderedCompare>$a, -$b</math>
</setup>

<p>
  <row>
    Find the critical numbers of the function
    <me>f(x) = $f.</me>
  </row>
  <row>
    Answer (separate by commas):
    <m>x = </m>
    <answer><award unorderedCompare>$ans</award></answer>
  </row>
</p>

<solution>
  Answer: $ans
</solution>
