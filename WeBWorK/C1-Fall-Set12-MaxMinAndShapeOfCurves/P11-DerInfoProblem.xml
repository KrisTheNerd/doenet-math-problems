<setup>
  <selectFromSequence from="-5" to="5" exclude="0" name="a" />
  <selectFromSequence from="1" to="4" name="b" />
  <math name="b1" simplify>3*(2*$b-1)</math>
  <math name="b2" simplify>6*$b</math>
  <function symbolic formula="4*x^3+$b1*x^2-$b2*x+$a" name="f" />
  <math name="c" simplify>$$f(-$b)</math>
  <math name="d" simplify>$$f(1/2)</math>
  <math name="e" simplify>(1-2*$b)/4</math>
</setup>

<copy f="$f"
  ans1="(-infinity,-$b)union(1/2,infinity)"
  ans2="(-$b,1/2)"
  ans3="(-$b,$c)"
  ans4="(1/2,$d)"
  ans5="($e, infinity)"
  ans6="(-infinity,$e)"
